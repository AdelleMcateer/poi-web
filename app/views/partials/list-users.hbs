<div class="uk-margin uk-width-2xlarge uk-margin-auto uk-card uk-card-default uk-card-body uk-box-shadow-large">
    <table class="uk-table">
        <caption>
            Islands of Ireland Admin Dashboard - List Users
        </caption>
        <thead>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
        <th>User ID</th>
        <th>Delete User</th>
        <th></th>
        </thead>
        <tbody class="uk-text-left">
        {{#each users}}
            <tr>
                <td>{{firstName}}</td>
                <td>{{lastName}}</td>
                <td>{{email}}</td>
                <th>{{id}}</th>
                <td><a href="/delete-user/{{_id}}" uk-icon="icon: trash"></a></td>
            </tr>
        {{/each}}
        </tbody>
    </table>
</div>